<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>
		<locale-config>
     		<default-locale>lt_LT</default-locale>
		</locale-config>
	   	<resource-bundle>
			<base-name>com.mAdmin.Labels</base-name>
			<var>label</var>
	   	</resource-bundle>
	   	<resource-bundle>
			<base-name>com.mAdmin.CalendarLabels</base-name>
			<var>calendarLabel</var>
	   	</resource-bundle>
	   	<resource-bundle>
			<base-name>com.mAdmin.Messages</base-name>
			<var>msg</var>
	   	</resource-bundle>
	   	<resource-bundle>
			<base-name>com.mAdmin.Roles</base-name>
			<var>roleLabel</var>
	   	</resource-bundle>
	   	<resource-bundle>
			<base-name>com.mAdmin.ClientStatuses</base-name>
			<var>clientStatus</var>
	   	</resource-bundle>
	   	<resource-bundle>
			<base-name>com.mAdmin.ContractTypes</base-name>
			<var>contractType</var>
	   	</resource-bundle>
		<resource-bundle>
			<base-name>com.mAdmin.AdminUTF8Bundle</base-name>
			<var>adm</var>
		</resource-bundle>
		<message-bundle>com.mAdmin.ApplicationMessages</message-bundle>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>	  
        <el-resolver>
			org.primefaces.application.exceptionhandler.PrimeExceptionHandlerELResolver
 		</el-resolver>  
	</application>
	<factory>
		<exception-handler-factory>
			org.omnifaces.exceptionhandler.FullAjaxExceptionHandlerFactory
		</exception-handler-factory>
	</factory>
    <lifecycle>
		<phase-listener id="AjaxTimeoutPhaseListener">com.mAdmin.security.AjaxTimeoutPhaseListener</phase-listener>
	</lifecycle>
</faces-config>